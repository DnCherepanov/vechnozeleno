                            <td valign='top'>
                              <table
                                width='100%'
                                border='0'
                                cellspacing='0'
                                cellpadding='0'
                                align='center'
                              >
                                <tr>
                                  <td width='30' class='em_hide'>&nbsp;</td>
                                  <td
                                    width='265'
                                    valign='top'
                                    align='left'
                                    style="
                                    font-family: 'Open Sans', Arial, sans-serif;
                                    font-size: 17px;
                                    line-height: 24px;
                                    color: #30373b;
                                  "
                                    class='em_font1'
                                  >
                                    {{title}}
                                  </td>
                                  <td width='5'></td>
                                  <td
                                    width='65'
                                    class='em_width55'
                                    valign='top'
                                    align='center'
                                    style="
                                    font-family: 'Open Sans', Arial, sans-serif;
                                    font-size: 17px;
                                    line-height: 24px;
                                    color: #30373b;
                                  "
                                  >
                                    {{quantity}}
                                    шт.
                                  </td>
                                  <td width='138' class='em_hide'></td>
                                  <td width='5'></td>
                                  <td
                                    align='right'
                                    valign='top'
                                    class='em_width75'
                                    style="
                                    font-family: 'Open Sans', Arial, sans-serif;
                                    font-size: 17px;
                                    line-height: 24px;
                                    color: #30373b;
                                  "
                                  >
                                    {{price}}
                                    &#8381;
                                  </td>
                                  <td width='30' class='em_hide'>&nbsp;</td>
                                </tr>
                              </table>
                            </td>
                          </tr>
                          <tr>
                            <td
                              height='8'
                              style='font-size: 1px; line-height: 1px'
                            >
                              &nbsp;
                            </td>
                          </tr>
                        {{/each}}
                        <tr>
                          <td
                            height='14'
                            style='font-size: 1px; line-height: 1px'
                          >
                            &nbsp;
                          </td>
                        </tr>
                        <tr>
                          <td
                            height='8'
                            class='em_hide'
                            style='font-size: 1px; line-height: 1px'
